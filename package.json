{
  "name": "sdk-js",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "configs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-coinvise": "workspace:*",
    "@coinvise/eslint-next": "workspace:*",
    "@coinvise/lint-staged": "workspace:*",
    "@coinvise/prettier": "workspace:*",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.2.1"
}
